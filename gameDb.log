/*C3*/SET SCHEMA PUBLIC
CREATE TABLE IF NOT EXISTS player(id INT NOT NULL PRIMARY KEY GENERATED ALWAYS AS IDENTITY(START WITH 1, INCREMENT BY 1), name VARCHAR(255) NOT NULL, wood INT NOT NULL, stone INT NOT NULL, iron INT NOT NULL)
CREATE TABLE IF NOT EXISTS village(id INT NOT NULL PRIMARY KEY GENERATED ALWAYS AS IDENTITY(START WITH 1, INCREMENT BY 1), player INT NOT NULL, communityHalls INT NOT NULL, sawmills INT NOT NULL, quarrys INT NOT NULL, mines INT NOT NULL, storages INT NOT NULL, apartments INT NOT NULL, walls INT NOT NULL)
CREATE TABLE IF NOT EXISTS buildings(id INT NOT NULL PRIMARY KEY GENERATED ALWAYS AS IDENTITY(START WITH 1, INCREMENT BY 1), village INT NOT NULL, type INT NOT NULL, xValue INT NOT NULL, yValue INT NOT NULL, level INT NOT NULL,number INT NOT NULL, additionalInformation BIGINT)
